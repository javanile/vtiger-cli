{
  "name": "javanile/vtiger-cli",
  "license": "MIT",
  "type": "project",
  "description": "Command-line utility for Vtiger CRM.",
  "homepage": "https://bref.sh",
  "keywords": ["vtiger", "command-line", "automation", "vtiger-extension", "crm"],
  "bin": [
    "vtiger"
  ],
  "autoload": {
    "psr-4": {
      "Javanile\\VtigerCli\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Javanile\\VtigerCli\\Tests\\": "tests",
      "Javanile\\VtigerCli\\Tests\\Fixtures\\": "tests/fixtures"
    }
  },
  "require": {
    "mnapoli/silly": "^1.5",
    "javanile/mysql-import": "^0.0.5"
  },
  "require-dev": {
    "phpunit/phpunit": "^5"
  }
}
